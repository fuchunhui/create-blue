{
  "name": "vite-create",
  "version": "1.0.0",
  "license": "MIT",
  "author": "fuchunhui",
  "main": "index.js",
  "bin": {
    "create-app": "index.js"
  },
  "files": [
    "index.js",
    "template-*"
  ],
  "scripts": {
    "changelog": "",
    "release": "node /scripts/release.js --skipBuild",
    "commitlint": "node scripts/verify-commit.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "enquirer": "^2.3.6",
    "kolorist": "^1.2.9",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "^4.1.1",
    "eslint": "^7.25.0",
    "eslint-define-config": "^1.0.8",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuchunhui/vite-create.git"
  },
  "keywords": [
    "vite",
    "create"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,html,css,md}": "prettier --write",
    "*.ts": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/fuchunhui/vite-create/issues"
  },
  "homepage": "https://github.com/fuchunhui/vite-create#readme",
