{
  "name": "vite-create",
  "version": "1.0.0",
  "license": "MIT",
  "author": "fuchunhui",
  "main": "index.js",
  "bin": {
    "create-app": "index.js"
  },
  "files": [
    "index.js",
    "template-*"
  ],
  "scripts": {
    "changelog": "",
    "release": "node /scripts/release.js --skipBuild"
  },
  "dependencies": {
    "enquirer": "^2.3.6",
    "kolorist": "^1.2.9",
    "minimist": "^1.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuchunhui/vite-create.git"
  },
  "keywords": [
    "vite",
    "create"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint",
      "prettier --parser=typescript --write"
    ],
    "*.html": [
      "prettier --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/fuchunhui/vite-create/issues"
  },
  "homepage": "https://github.com/fuchunhui/vite-create#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
